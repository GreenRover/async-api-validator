{
    "compilerOptions": {
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        "outDir": "./dist",
        "rootDir": "./",
        "sourceMap": true,
        "target": "es2018",
        "module": "commonjs",
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true
    },
    "exclude": [
        "logs",
        "node_modules",
        "cypress"
    ]
}
